{
	"name": "zurememes",
	"private": "true",
	"scripts": {
		"build": "astro build",
		"dev": "astro dev",
		"lint:casing": "case-police --ignore '**/*.d.ts'",
		"lint:casing:fix": "npm run lint:casing --fix",
		"lint:scripts": "eslint '**/*.{astro,js,mjs,json,ts,vue}'",
		"lint:scripts:fix": "npm run eslint -- --fix",
		"lint:styles": "stylelint '**/*.{css,vue}'",
		"lint:styles:fix": "npm run stylelint -- --fix",
		"lint:types": "npm run lint:types:vue",
		"lint:types:astro": "astro check",
		"lint:types:vue": "vue-tsc",
		"netlify": "netlify",
		"workflow:build": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --minify --outfile=workflow/export/index.js",
		"workflow:dev": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --watch --outfile=workflow/export/index.js"
	},
	"dependencies": {
		"@astrojs/netlify": "3.0.3",
		"@astrojs/vue": "3.0.2",
		"@egjs/vue3-infinitegrid": "4.10.1",
		"@nanostores/vue": "0.10.0",
		"@netlify/functions": "2.3.0",
		"@supabase/ssr": "0.0.5",
		"@supabase/supabase-js": "2.38.4",
		"@types/js-cookie": "3.0.5",
		"@types/node": "18.18.7",
		"@unocss/astro": "0.57.1",
		"@unocss/preset-uno": "0.57.1",
		"@unocss/reset": "0.57.1",
		"@unocss/transformer-directives": "0.57.1",
		"@vite-pwa/astro": "0.1.4",
		"@vuelidate/core": "2.0.3",
		"@vuelidate/validators": "2.0.4",
		"astro": "3.4.0",
		"colord": "2.9.3",
		"filenamify-url": "3.0.0",
		"fzf": "0.5.2",
		"js-cookie": "3.0.5",
		"nanoid": "5.0.2",
		"nanostores": "0.9.4",
		"node-fetch": "3.3.2",
		"unocss": "0.57.1"
	},
	"devDependencies": {
		"@commitlint/cli": "17.8.1",
		"@commitlint/config-conventional": "17.8.1",
		"@zeebats/eslint-config": "1.4.0-beta.7",
		"@zeebats/stylelint-config": "1.3.0",
		"c8": "8.0.1",
		"case-police": "0.6.1",
		"lint-staged": "15.0.2",
		"netlify-cli": "16.9.3",
		"postcss-nested": "6.0.1",
		"postcss-nested-ancestors": "3.0.0",
		"simple-git-hooks": "2.9.0",
		"vitest": "0.34.6",
		"vue-tsc": "1.8.22"
	},
	"overrides": {
		"vue": "3"
	}
}
