{
	"name": "zurememes",
	"private": "true",
	"scripts": {
		"build": "astro build",
		"check:case": "case-police --ignore '**/*.d.ts'",
		"check:case:fix": "npm run check:case --fix",
		"check:types": "npm run check:types:vue",
		"check:types:astro": "astro check",
		"check:types:vue": "vue-tsc",
		"dev": "astro dev",
		"eslint": "eslint '**/*.{js,mjs,json,ts,vue}'",
		"eslint:fix": "npm run eslint -- --fix",
		"netlify": "netlify",
		"stylelint": "stylelint '**/*.{css,vue}'",
		"stylelint:fix": "npm run stylelint -- --fix",
		"workflow:build": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --minify --outfile=workflow/export/index.js",
		"workflow:dev": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --watch --outfile=workflow/export/index.js"
	},
	"dependencies": {
		"@astrojs/netlify": "2.2.0",
		"@astrojs/vue": "2.1.0",
		"@egjs/vue3-infinitegrid": "4.9.0",
		"@nanostores/vue": "0.7.0",
		"@netlify/functions": "1.4.0",
		"@supabase/supabase-js": "2.12.0",
		"@types/js-cookie": "3.0.3",
		"@types/node": "18.15.5",
		"@unocss/astro": "0.50.6",
		"@unocss/preset-uno": "0.50.6",
		"@unocss/reset": "0.50.6",
		"@unocss/transformer-directives": "0.50.6",
		"@vite-pwa/astro": "0.0.5",
		"@vuelidate/core": "2.0.0",
		"@vuelidate/validators": "2.0.0",
		"astro": "2.1.5",
		"colord": "2.9.3",
		"filenamify-url": "3.0.0",
		"fzf": "0.5.1",
		"js-cookie": "3.0.1",
		"nanoid": "4.0.1",
		"nanostores": "0.7.4",
		"node-fetch": "3.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "17.5.0",
		"@commitlint/config-conventional": "17.4.4",
		"@zeebats/eslint-config": "1.3.1",
		"@zeebats/stylelint-config": "1.1.1",
		"c8": "7.13.0",
		"case-police": "0.5.14",
		"netlify-cli": "13.2.1",
		"postcss-nested": "6.0.1",
		"postcss-nested-ancestors": "3.0.0",
		"simple-git-hooks": "2.8.1",
		"vitest": "0.29.7",
		"vue-tsc": "1.2.0"
	},
	"overrides": {
		"vue": "3"
	}
}
