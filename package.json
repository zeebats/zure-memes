{
	"name": "zurememes",
	"private": "true",
	"scripts": {
		"build": "nuxi build",
		"check:case": "case-police",
		"check:case:fix": "npm run check:case --fix",
		"check:types": "vue-tsc",
		"dev": "astro dev",
		"eslint": "eslint '**/*.{json,ts,vue}'",
		"eslint:fix": "npm run eslint -- --fix",
		"generate": "nuxt generate",
		"netlify": "netlify",
		"preview": "nuxt preview",
		"stylelint": "stylelint '**/*.{css,vue}'",
		"stylelint:fix": "npm run stylelint -- --fix",
		"workflow:build": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --minify --outfile=workflow/export/index.js",
		"workflow:dev": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --watch --outfile=workflow/export/index.js"
	},
	"dependencies": {
		"@astrojs/netlify": "^1.2.1",
		"@astrojs/vue": "1.2.1",
		"@egjs/vue3-infinitegrid": "4.7.1",
		"@nanostores/vue": "0.6.1",
		"@netlify/functions": "1.3.0",
		"@supabase/supabase-js": "2.1.0",
		"@types/node": "18.11.9",
		"@unocss/astro": "^0.46.5",
		"@unocss/preset-uno": "0.46.5",
		"@unocss/reset": "0.46.5",
		"@unocss/transformer-directives": "0.46.5",
		"@vuelidate/core": "2.0.0",
		"@vuelidate/validators": "2.0.0",
		"astro": "1.6.7",
		"colord": "2.9.3",
		"filenamify-url": "3.0.0",
		"fzf": "0.5.1",
		"nanoid": "4.0.0",
		"nanostores": "0.7.1",
		"node-fetch": "3.3.0",
		"pinia": "2.0.23",
		"postcss-nested": "6.0.0",
		"postcss-nested-ancestors": "3.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.2.0",
		"@commitlint/config-conventional": "17.2.0",
		"@zeebats/eslint-config": "1.3.1",
		"@zeebats/stylelint-config": "1.1.0",
		"c8": "7.12.0",
		"case-police": "0.5.10",
		"esbuild": "0.15.13",
		"netlify-cli": "12.1.1",
		"simple-git-hooks": "2.8.1",
		"vitest": "0.25.1"
	},
