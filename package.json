{
	"name": "zurememes",
	"private": "true",
	"scripts": {
		"build": "astro build",
		"dev": "astro dev",
		"lint:casing": "case-police --ignore '**/*.d.ts'",
		"lint:casing:fix": "npm run lint:casing --fix",
		"lint:scripts": "eslint '**/*.{astro,js,mjs,json,ts,vue}'",
		"lint:scripts:fix": "npm run eslint -- --fix",
		"lint:styles": "stylelint '**/*.{css,vue}'",
		"lint:styles:fix": "npm run stylelint -- --fix",
		"lint:types": "npm run lint:types:vue",
		"lint:types:astro": "astro check",
		"lint:types:vue": "vue-tsc",
		"netlify": "netlify",
		"workflow:build": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --minify --outfile=workflow/export/index.js",
		"workflow:dev": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --watch --outfile=workflow/export/index.js"
	},
	"dependencies": {
		"@astrojs/netlify": "3.1.1",
		"@astrojs/vue": "4.0.8",
		"@egjs/vue3-infinitegrid": "4.11.0",
		"@nanostores/vue": "0.10.0",
		"@netlify/functions": "2.5.1",
		"@supabase/ssr": "0.0.10",
		"@supabase/supabase-js": "2.39.3",
		"@types/js-cookie": "3.0.6",
		"@types/node": "20.11.5",
		"@unocss/astro": "0.58.3",
		"@unocss/preset-uno": "0.58.3",
		"@unocss/reset": "0.58.3",
		"@unocss/transformer-directives": "0.58.3",
		"@vite-pwa/astro": "0.2.0",
		"@vuelidate/core": "2.0.3",
		"@vuelidate/validators": "2.0.4",
		"astro": "4.2.3",
		"colord": "2.9.3",
		"filenamify-url": "3.1.0",
		"fzf": "0.5.2",
		"js-cookie": "3.0.5",
		"nanoid": "5.0.4",
		"nanostores": "0.9.5",
		"node-fetch": "3.3.2",
		"unocss": "0.58.3",
		"vite-plugin-pwa": "0.17.4",
		"workbox-window": "7.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "18.5.0",
		"@commitlint/config-conventional": "18.5.0",
		"@zeebats/eslint-config": "1.4.0-beta-9",
		"@zeebats/stylelint-config": "1.3.0",
		"c8": "9.1.0",
		"case-police": "0.6.1",
		"lint-staged": "15.2.0",
		"netlify-cli": "17.15.2",
		"postcss-nested": "6.0.1",
		"postcss-nested-ancestors": "3.0.0",
		"simple-git-hooks": "2.9.0",
		"vitest": "1.2.1",
		"vue-tsc": "1.8.27"
	},
	"overrides": {
		"vue": "3"
	}
}
