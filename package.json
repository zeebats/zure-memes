{
	"name": "zurememes",
	"private": "true",
	"scripts": {
		"build": "astro build",
		"check:case": "case-police --ignore '**/*.d.ts'",
		"check:case:fix": "npm run check:case --fix",
		"check:types": "npm run check:types:vue",
		"check:types:astro": "astro check",
		"check:types:vue": "vue-tsc",
		"dev": "astro dev",
		"eslint": "eslint '**/*.{js,mjs,json,ts,vue}'",
		"eslint:fix": "npm run eslint -- --fix",
		"netlify": "netlify",
		"stylelint": "stylelint '**/*.{css,vue}'",
		"stylelint:fix": "npm run stylelint -- --fix",
		"workflow:build": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --minify --outfile=workflow/export/index.js",
		"workflow:dev": "esbuild workflow/index.ts --bundle --platform=node --target=node18 --watch --outfile=workflow/export/index.js"
	},
	"dependencies": {
		"@astrojs/netlify": "3.0.1",
		"@astrojs/vue": "3.0.0",
		"@egjs/vue3-infinitegrid": "4.10.1",
		"@nanostores/vue": "0.10.0",
		"@netlify/functions": "2.0.2",
		"@supabase/supabase-js": "2.36.0",
		"@types/js-cookie": "3.0.4",
		"@types/node": "20.8.9",
		"@unocss/astro": "0.56.1",
		"@unocss/preset-uno": "0.56.1",
		"@unocss/reset": "0.56.1",
		"@unocss/transformer-directives": "0.56.1",
		"@vite-pwa/astro": "0.1.3",
		"@vuelidate/core": "2.0.3",
		"@vuelidate/validators": "2.0.4",
		"astro": "3.1.2",
		"colord": "2.9.3",
		"filenamify-url": "3.0.0",
		"fzf": "0.5.2",
		"js-cookie": "3.0.5",
		"nanoid": "5.0.1",
		"nanostores": "0.9.3",
		"node-fetch": "3.3.2"
	},
	"devDependencies": {
		"@commitlint/cli": "17.7.1",
		"@commitlint/config-conventional": "17.7.0",
		"@zeebats/eslint-config": "1.3.1",
		"@zeebats/stylelint-config": "1.3.0",
		"c8": "8.0.1",
		"case-police": "0.6.1",
		"netlify-cli": "16.4.2",
		"postcss-nested": "6.0.1",
		"postcss-nested-ancestors": "3.0.0",
		"simple-git-hooks": "2.9.0",
		"vitest": "0.34.5",
		"vue-tsc": "1.8.13"
	},
	"overrides": {
		"vue": "3"
	}
}
