{
	"extends": [
		"@zeebats/eslint-config/vue3",
		"@zeebats/eslint-config/typescript"
	],
	"ignorePatterns": [
		"coverage/**/*",
		"src/env.d.ts",
		"workflow/export/**/*"
	],
	"overrides": [
		{
			"files": ["*.d.ts"],
			"rules": {
				"@typescript-eslint/no-unused-vars": "off",
				"unicorn/prevent-abbreviations": "off"
			}
		}
	]
}
